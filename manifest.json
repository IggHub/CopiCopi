{
    "manifest_version": 2,
    "name": "CopiCopi"",
    "description": "Easily copy messages from browser",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["myScript.js"]
        }
    ]   
}
